  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "vitest",
    "test:watch": "vitest watch",
    "test:cov": "vitest run --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/vitest",
    "test:e2e": "vitest run --config ./test/vitest-e2e.json",
    "db:migrate": "migrate-mongo up",
    "db:migrate:create": "migrate-mongo create -f migrate-mongo-config.js",
    "db:migrate:status": "migrate-mongo status -f migrate-mongo-config.js",
    "db:migrate:up": "migrate-mongo up -f migrate-mongo-config.js",
    "db:migrate:down": "migrate-mongo down -f migrate-mongo-config.js"
  },
