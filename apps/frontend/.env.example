# API Configuration
VITE_API_URL=http://localhost:3001
VITE_API_TIMEOUT=30000

# App Configuration
VITE_APP_NAME=Fresh Expense
VITE_APP_VERSION=1.0.0
VITE_ENVIRONMENT=development

# Authentication
VITE_JWT_EXPIRES_IN=24h

# Feature Flags
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_NOTIFICATIONS=true
VITE_ENABLE_GOOGLE_AUTH=true
VITE_ENABLE_TELLER_INTEGRATION=true
VITE_ENABLE_RECEIPT_UPLOAD=true

# UI Configuration
VITE_TOAST_DURATION=5000
VITE_ANIMATION_DURATION=300
VITE_DEBOUNCE_DELAY=300
VITE_MOBILE_BREAKPOINT=768

# External Services
VITE_GOOGLE_CLIENT_ID=your_google_client_id
VITE_SENTRY_DSN=your-sentry-dsn

# Development Settings
VITE_DEV_SERVER_PORT=3001
VITE_ENABLE_DEBUG=true

# Security
VITE_JWT_STORAGE_KEY=fresh_expense_jwt
VITE_REFRESH_TOKEN_KEY=fresh_expense_refresh_token

# Storage Configuration
VITE_R2_PUBLIC_URL=http://localhost:3001/storage
VITE_R2_BUCKET_NAME=dev-bucket

# Teller API Configuration
VITE_TELLER_APPLICATION_ID=your_teller_app_id
VITE_TELLER_ENVIRONMENT=development
VITE_TELLER_API_URL=http://localhost:3001/teller
VITE_TELLER_WEBHOOK_SECRET=your_webhook_secret

# Google OAuth Configuration
VITE_GOOGLE_REDIRECT_URI=http://localhost:5173/auth/google/callback 