  "scripts": {
    "start:dev": "nx serve backend",
    "build": "nx build backend",
    "test": "nx test backend",
    "lint": "nx lint backend",
    "format": "prettier --write \"src/**/*.ts\"",
    "serve": "nx serve",
    "start": "nest start",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "db:migrate": "migrate-mongo up -f config/migrate-mongo-config.js",
    "db:migrate:create": "migrate-mongo create -f config/migrate-mongo-config.js",
    "db:migrate:status": "migrate-mongo status -f config/migrate-mongo-config.js",
    "db:migrate:up": "migrate-mongo up -f config/migrate-mongo-config.js",
    "db:migrate:down": "migrate-mongo down -f config/migrate-mongo-config.js"
  },
