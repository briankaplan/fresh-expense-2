{
  "name": "@fresh-expense/types",
  "version": "0.0.1",
  "description": "Shared TypeScript types for Fresh Expense",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest",
    "watch": "tsc -w"
  },
  "dependencies": {
    "@nestjs/common": "^10.0.0",
    "@nestjs/mongoose": "^10.0.0",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.1",
    "mongoose": "^7.6.8",
    "reflect-metadata": "0.1.13"
  },
  "devDependencies": {
    "@types/node": "^18.15.11",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "@vitest/coverage-v8": "1.6.1",
    "eslint": "^8.37.0",
    "rimraf": "^4.4.1",
    "tsx": "^4.19.3",
    "typescript": "^5.0.3",
    "vitest": "1.6.1"
  }
}