    "@types/bcrypt": "5.0.2",
    "@types/chart.js": "^2.9.41",
    "@types/compression": "1.7.5",
    "@types/express": "^4.17.21",
    "@types/express-serve-static-core": "^4.19.6",
    "@types/fs-extra": "^11.0.4",
    "@types/glob": "^8.1.0",
    "@types/jest": "^29.5.14",
    "@types/js-yaml": "^4.0.9",
    "@types/jsdom": "^21.1.7",
    "@types/jsonwebtoken": "9.0.5",
    "@types/mime-types": "^2.1.4",
    "@types/mongodb": "^4.0.7",
    "@types/multer": "^1.4.12",
    "@types/node": "22.14.1",
    "@types/nodemailer": "6.4.14",
