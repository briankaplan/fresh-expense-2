  "scripts": {
    "build": "pnpm run build:packages && pnpm run build:apps",
    "build:packages": "pnpm -r --filter @fresh-expense/types --filter @fresh-expense/utils --filter @fresh-expense/hooks --filter @fresh-expense/ui run build",
    "build:apps": "pnpm -r --filter @fresh-expense/backend --filter @fresh-expense/frontend run build",
    "start:dev": "pnpm --filter=./apps/backend dev",
    "start:dev:frontend": "pnpm --filter=./apps/frontend dev",
    "test": "pnpm test:backend && pnpm test:frontend",
    "test:backend": "pnpm --filter=./apps/backend test",
    "test:frontend": "pnpm --filter=./apps/frontend test",
    "test:watch": "pnpm --filter=./apps/backend test:watch",
    "test:cov": "pnpm --filter=./apps/backend test:cov && pnpm --filter=./apps/frontend test:cov",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "clean": "pnpm -r run clean",
    "fix": "find . -type d -name dist -exec rm -rf {} + && pnpm exec eslint . --fix && pnpm exec prettier --write .",
    "lint:big": "NODE_OPTIONS='--max-old-space-size=8192' eslint .",
    "lint:fix:big": "NODE_OPTIONS='--max-old-space-size=8192' eslint . --fix",
    "lint:fix:massive": "NODE_OPTIONS='--max-old-space-size=8192' eslint apps/backend/**/*.{ts,js} --fix",
    "lint:workspaces": "node scripts/lint-workspaces.js",
    "lint:fix:vars": "tsx scripts/fix-unused-vars.ts",
    "lint:clean": "pnpm run lint:fix:massive && pnpm run lint:fix:vars"
  },
