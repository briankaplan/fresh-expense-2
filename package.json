    "fs-extra": "^11.3.0",
    "glob": "^11.0.1",
    "google-auth-library": "^9.15.1",
    "husky": "^9.1.7",
    "jest": "29.7.0",
    "jscpd": "^4.0.5",
    "lint-staged": "^15.5.1",
    "mongodb": "6.3.0",
    "nx": "18.0.0",
    "prettier": "3.5.3",
    "prettier-plugin-packagejson": "^2.5.10",
    "ts-morph": "^25.0.1",
    "ts-node": "10.9.2",
    "ts-prune": "^0.10.3",
    "tsx": "^4.19.3",
    "type-coverage": "^2.29.7",
    "typescript": "5.8.3",
    "vite-plugin-dts": "^4.5.3",
    "vite-plugin-static-copy": "2.3.1",
    "webpack": "5.99.5"
  },
  "packageManager": "pnpm@8.15.4",
  "pnpm": {
