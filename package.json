{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "fresh-expense-monorepo",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "workspaces": ["packages/*", "apps/*", "apps/workers/*"],
  "overrides": {
    "@nestjs/common": "10.0.0",
    "@nestjs/core": "10.0.0",
    "@nestjs/platform-express": "10.0.0",
    "@nestjs/mongoose": "10.0.0",
    "@nestjs/swagger": "7.4.2",
    "@nestjs/passport": "10.0.3",
    "@nestjs/terminus": "10.0.1",
    "@nestjs/cache-manager": "2.3.0",
    "mongoose": "7.6.8",
    "reflect-metadata": "0.1.13"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/briankaplan/fresh-expense-2.git"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "scripts": {
    "build": "pnpm run build:packages && pnpm run build:apps",
    "build:packages": "pnpm run build -w @fresh-expense/types -w @fresh-expense/utils -w @fresh-expense/hooks -w @fresh-expense/ui",
    "build:apps": "pnpm run build -w apps/backend -w apps/frontend",
    "start:dev": "pnpm --filter=./apps/backend dev",
    "start:dev:frontend": "pnpm --filter=./apps/frontend dev",
    "test": "pnpm test:backend && pnpm test:frontend",
    "test:backend": "pnpm --filter=./apps/backend test",
    "test:frontend": "pnpm --filter=./apps/frontend test",
    "test:watch": "pnpm --filter=./apps/backend test:watch",
    "test:cov": "pnpm --filter=./apps/backend test:cov && pnpm --filter=./apps/frontend test:cov",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "clean": "pnpm -r run clean"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "3.525.0",
    "@aws-sdk/s3-request-presigner": "3.525.0",
    "@cloudflare/workers-types": "^4.20250416.0",
    "@nestjs-modules/ioredis": "^2.0.2",
    "@nestjs/bull": "^11.0.2",
    "@nestjs/common": "10.0.0",
    "@nestjs/config": "4.0.2",
    "@nestjs/core": "10.0.0",
    "@nestjs/event-emitter": "^3.0.1",
    "@nestjs/mongoose": "10.0.0",
    "@nestjs/platform-express": "10.0.0",
    "@nestjs/schedule": "5.0.1",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.1",
    "compression": "1.7.4",
    "express": "4.20.0",
    "helmet": "7.1.0",
    "itty-router": "^5.0.18",
    "jsonwebtoken": "9.0.2",
    "mongoose": "7.6.8",
    "node-cache": "^5.1.2",
    "node-fetch": "3.3.2",
    "nodemailer": "6.9.10",
    "pdf-img-convert": "1.2.1",
    "pdfkit": "0.14.0",
    "reflect-metadata": "0.1.13",
    "sharp": "0.33.2",
    "string-similarity-js": "^2.1.4",
    "tesseract.js": "5.0.5",
    "uuid": "9.0.1",
    "zod": "3.22.4"
  },
  "devDependencies": {
    "@babel/core": "^7.26.10",
    "@babel/plugin-transform-runtime": "^7.26.10",
    "@babel/preset-env": "^7.26.9",
    "@babel/preset-react": "^7.26.3",
    "@babel/preset-typescript": "^7.27.0",
    "@babel/runtime": "^7.27.0",
    "@biomejs/biome": "^1.9.4",
    "@google-cloud/storage": "^7.16.0",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/passport": "10.0.3",
    "@nx/devkit": "18.0.0",
    "@nx/eslint": "18.0.0",
    "@nx/eslint-plugin": "18.0.0",
    "@nx/jest": "18.0.0",
    "@nx/js": "18.0.0",
