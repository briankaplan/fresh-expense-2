{
  "name": "my-vite-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit",
    "depcruise": "npx dependency-cruiser src --include-only '^src' --output-type err-long",
    "docs": "typedoc --out docs src/main.tsx",
    "prepare": "husky install",
    "pre-commit": "lint-staged"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@types/node": "^22.10.7",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "@vitejs/plugin-react": "^4.3.4",
    "dependency-cruiser": "^15.5.0",
    "eslint": "^9.17.0",
    "eslint-plugin-folders": "^1.0.4",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "globals": "^15.14.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "typedoc": "^0.25.7",
    "typescript": "^5.7.3",
    "vite": "^6.0.5"
  }
}
